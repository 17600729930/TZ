.video_wrap{
  width: 100%;
  box-sizing: border-box;
  padding: 0 8rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.video_wrap .video_box{
  width: 49%;
  border-radius: 5px;
  border: 1px solid #f5f5f5;
  margin-bottom: 16rpx;
}
.video_wrap .video_box video{
  width: 100%;
  max-height: 285rpx;
}
.video_wrap .video_box .title{
  font-size: 30rpx;
  color: #000;
}
.video_wrap .video_box .view{
  font-size:24rpx;
  margin: 10rpx 0;
  color:#555; 
}
