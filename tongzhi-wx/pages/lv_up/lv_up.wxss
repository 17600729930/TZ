.jubao{
  width: 100%;
}
.jubao .why{
  box-sizing: border-box;
  padding-left: 18rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #f6f7fa;
  font-size: 30rpx;
  color: #999;
}
.jubao .wrap{
  box-sizing: border-box;
  padding: 0 18rpx;
  background: #fff;
  margin-bottom: 110rpx;
}
.jubao .wrap .vip{
  width: 100%;
  padding-top: 20rpx;
}
.jubao .wrap .vip image{
  width: 100%;
}
.jubao .wrap .price{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border: 1px solid #f3f3f3;
  padding: 20rpx 30rpx;
  margin-top: 10rpx;
  border-radius: 3px;
}
.jubao .wrap .active{
  border: 1px solid #ff6006;
  background: #fdfbef;
}
.jubao .wrap .price .left{
  width: 50%;
}
.jubao .wrap .price .left text{
  font-size: 36rpx;
}
.jubao .wrap .price .left view{
  margin-top: 10rpx;
  width: 100%;
  font-size: 32rpx;
  color: #333;
}
.jubao .wrap .price .left view text{
  color: #ff6906;
  font-size: 32rpx;
  margin-left: 20rpx;
}
.jubao .wrap .price .right{
  width: 50%;
}
.jubao .wrap .price .right view{
  text-align: right;
  color:#ff6006;
  font-size: 30rpx;
  font-weight: 700;
}
.jubao .wrap .price .right view text{
  font-size: 400;
  color: #a5a5a5;
}
.jubao .te{
  width: 100%;
  padding: 20rpx 0;
}
.jubao .te .te_wrap{
  width: 100%;
  margin-top: 20rpx;
}
.jubao .te .te_wrap .top{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.jubao .te .te_wrap .top image{
  width: 50rpx;
}
.jubao .te .te_wrap .top text{
  font-size: 34rpx;
  color: #333;
  padding-left: 20rpx;
}
.jubao .te .te_wrap .bot{
  padding-left: 70rpx;
  font-size: 32rpx;
  color: #ceccd5;
}
.pay{
  width: 100%;
  height: 110rpx;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.pay .left{
  width: 70%;
  height: 100%;
  background: #f5f6fb;
}
.pay .left text{
  line-height: 110rpx;
  text-align: center;
  width: 100%;
  display: inline-block;
  font-size: 32rpx;
  font-weight: 700;
}
.pay .right{
  width: 30%;
  height: 100%;
  background: #ff6906;  
}
.pay .right navigator{
  line-height: 110rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
}